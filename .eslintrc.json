{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "trybe-frontend",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-max-depth": 0,
        "max-len": 0
    }
}
